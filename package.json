{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "author": "pdeng",
  "scripts": {
    "serve": "vue-cli-service serve",
    "local": "vue-cli-service serve --mode local",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "ui:init": "et -i src/assets/style/element-variables.scss",
    "ui:theme": "et -c src/assets/style/element-variables.scss -o src/assets/style/theme/ -m --watch",
    "new:comp": "node ./script/generateComponent",
    "new:view": "node ./script/generateView"
  },
  "dependencies": {
    "@pdeng/x-charts": "0.0.3-0",
    "axios": "^0.19.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "^3.0.0",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.21",
    "echarts": "^4.6.0",
    "element-ui": "^2.13.0",
    "lodash": "^4.17.19",
    "nprogress": "^0.2.0",
    "svg-sprite-loader": "^4.2.1",
    "vue": "^2.6.11",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "compression-webpack-plugin": "^3.1.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "loadash": "^1.0.0",
    "mockjs": "^1.1.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.3.2",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        0,
        {
          "vars": "local",
          "args": "none"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
