{
  "name": "ctc-admin-tpl",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "deploy-cli-service deploy --mode dev",
    "new:comp": "node ./scripts/generate-component",
    "new:view": "node ./scripts/generate-view",
    "theme": "et -i src/assets/styles/element-variables.scss && et -c src/assets/styles/element-variables.scss -o src/assets/styles/theme/ -m --watch"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "echarts": "^5.0.1",
    "lodash": "^4.17.20",
    "ol": "^6.5.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@pdeng/element-table": "^0.2.7",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "deploy-cli-service": "^1.0.9",
    "element-theme-chalk": "^2.13.2",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "fs": "0.0.1-security",
    "lint-staged": "^9.5.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.1.0",
    "style-resources-loader": "^1.3.3",
    "svg-sprite-loader": "^5.0.0",
    "vue-cli-plugin-style-resources-loader": "~0.1.4",
    "vue-template-compiler": "^2.6.11",
    "vuex-persistedstate": "^3.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
